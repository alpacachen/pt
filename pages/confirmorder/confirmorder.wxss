/* pages/orderdetail/orderdetail.wxss */
.title{
  border-bottom: 1px solid #ccc;
  padding: 15px 0;
  margin: 0 15px;
  display: flex;
  justify-content: space-between;
}
.left{
  color: #999999;
  font-size: 12px;
  font-family: PingFangSC-Medium;
}
.right{
  display: inline-block;
  color: #666666;
  font-size: 14px;
}
.title .right image{
  display: inline-block;
  width: 40px;
  height: 40px;
  margin-right: 15px;
  vertical-align: middle;
  border-radius: 50%;
}
.con{
  padding: 15px 0 0 0;
  margin: 0 15px;
  border-bottom: 1px solid #ccc;
}
.con .line {
  margin-bottom: 25px;
  display: flex;
  justify-content: space-between;
}
.pay{
  padding: 20px 15px 100px 15px;
}
.avatars image{
  display: inline-block;
  width: 24px;
  height: 24px;
  margin: 10px 10px 0 0;
  border-radius: 50%;
}
.but{
  margin: 0 20px;
  background: #00bf4a;
  color: #fff;
  position: fixed;
  bottom: 50px;
  left: 0;
  right: 0;
}